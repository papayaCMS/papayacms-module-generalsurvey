<?xml version="1.0" encoding="ISO-8859-1" ?>
<table name="general_survey_answer" prefix="yes" type="transactions">
  <fields>
    <field name="answer_id" type="integer" size="4" null="no" autoinc="yes"/>
    <field name="question_id" type="integer" size="4" null="no"/>
    <field name="answer_title" type="string" size="255" null="no"/>
    <field name="answer_description" type="string" size="65535" null="no"/>
    <field name="answer_order" type="integer" size="4" null="no"/>
  </fields>
  <keys>
    <primary-key>
      <field>answer_id</field>
    </primary-key>
  </keys>
</table>
