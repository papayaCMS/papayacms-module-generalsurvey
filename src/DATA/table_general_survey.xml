<?xml version="1.0" encoding="ISO-8859-1" ?>
<table name="general_survey" prefix="yes" type="transactions">
  <fields>
    <field name="survey_id" type="integer" size="4" null="no" autoinc="yes"/>
    <field name="survey_title" type="string" size="255" null="no"/>
    <field name="survey_description" type="string" size="65535" null="no"/>
  </fields>
  <keys>
    <primary-key>
      <field>survey_id</field>
    </primary-key>
    <key name="survey_title">
      <field>survey_title</field>
    </key>
  </keys>
</table>
