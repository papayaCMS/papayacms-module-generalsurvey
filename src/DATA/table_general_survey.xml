    <field name="survey_id" type="integer" size="4" null="no" autoinc="yes"/>
    <field name="survey_use_subjects" type="integer" size="4" null="no"/>
    <field name="survey_use_answers" type="integer" size="4" null="no"/>
    <field name="deleted" type="integer" size="4" null="no"/>
  </fields>
  <keys>
    <primary-key>
      <field>survey_id</field>
    </primary-key>
    <key name="deleted">
      <field>deleted</field>
    </key>
  </keys>
</table>
